<p id="notice"><%= notice %></p>

<h1>Tagings</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Tagable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tagings.each do |taging| %>
      <tr>
        <td><%= taging.tag_id %></td>
        <td><%= taging.tagable_id %></td>
        <td><%= link_to 'Show', taging %></td>
        <td><%= link_to 'Edit', edit_taging_path(taging) %></td>
        <td><%= link_to 'Destroy', taging, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taging', new_taging_path %>
